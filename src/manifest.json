{
  "manifest_version": 3,
  "name": "SaveIt",
  "description": "Save your tabs for future use.",
  "version": "0.5.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["activeTab"],
  "host_permissions": ["<all_urls>"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
